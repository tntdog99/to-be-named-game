prefix=c:/lua-batteries
includedir=${prefix}/include
libdir=${prefix}/lib

Name: lua-openssl
Description: Openssl binding for Lua
URL: https://github.com/zhaozg/lua-openssl
Version: 0.8.3
Requires: luajit, openssl
Libs: -L${prefix}/lib/lua/5.1-static -llua.openssl -lws2_32 -lkernel32 -luser32 -lgdi32 -ladvapi32
Cflags: -I${includedir}
