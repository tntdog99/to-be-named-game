prefix=/usr/local/lua-batteries
includedir=${prefix}/include
libdir=${prefix}/lib

Name: luasocket
Description: Network support for the Lua language
URL: https://github.com/lunarmodules/luasocket
Version: 3.1.0
Requires: lua54
Libs: -L${prefix}/lib/lua/5.4-static -llua.socket.unix -llua.socket.serial -llua.mime.core -llua.socket.core -lm -ldl -lreadline
Cflags: -I${includedir}
