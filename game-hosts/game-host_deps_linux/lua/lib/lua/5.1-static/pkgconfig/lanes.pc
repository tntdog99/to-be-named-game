prefix=/usr/local/lua-batteries
includedir=${prefix}/include
libdir=${prefix}/lib

Name: lanes
Description: Multithreading support for Lua
URL: https://github.com/LuaLanes/lanes
Version: 3.16.0
Requires: luajit
Libs: -L${prefix}/lib/lua/5.1-static -llua.lanes.core -lm -ldl -pthread
Cflags: -I${includedir} -pthread
